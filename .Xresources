Xft.antilias: true
Xft.autohint: false
Xft.dpi: 96
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault
Xft.rgba: rgb

*.font: xft:Roboto Mono Nerd Font:style=medium:pixelsize=13
*.boldFont: xft:Roboto Mono Nerd Font:style=bold:pixelsize=13
*.ItalicFont: xft:Roboto Mono Nerd Font:style=italic:pixelsize=13
*.boldItalicfont: xft:Roboto Mono Nerd Font:style=boldItalic:pixelsize=13
! URxvt.font: xft:Iosevka NF:style=medium:pixelsize=15
! URxvt.font: xft:LiterationMonoPowerLine Nerd Font:style=medium:pixelsize=13


! URxvt.boldFont: xft:Inconsolata for Powerline Nerd Font:style=bold:pixelsize=15
! URxvt.ItalicFont: xft:Inconsolata for Powerline Nerd Font:style=italic:pixelsize=15
! URxvt.boldItalicFont: xft:Inconsolata for Powerline Nerd Font:style=bolditalic:pixelsize=15

! URxvt.letterSpace: -1

XTerm*reverseVideo: on
URxvt.scrollBar: false
URxvt.shading: 20
URxvt.cursorBlink: false

! Base16 Default Dark
! Scheme: Chris Kempson (http://chriskempson.com)
#define base00 #181818
#define base01 #282828
#define base02 #383838
#define base03 #585858
#define base04 #b8b8b8
#define base05 #d8d8d8
#define base06 #e8e8e8
#define base07 #f8f8f8
#define base08 #ab4642
#define base09 #dc9656
#define base0A #f7ca88
#define base0B #a1b56c
#define base0C #86c1b9
#define base0D #7cafc2
#define base0E #ba8baf
#define base0F #a16946

*.foreground:   base05
#ifdef background_opacity
*.background:   [background_opacity]base00
#else
*.background:   base00
#endif
*.cursorColor:  base05

*.color0:       base00
*.color1:       base08
*.color2:       base0B
*.color3:       base0A
*.color4:       base0D
*.color5:       base0E
*.color6:       base0C
*.color7:       base05

*.color8:       base03
*.color9:       base08
*.color10:      base0B
*.color11:      base0A
*.color12:      base0D
*.color13:      base0E
*.color14:      base0C
*.color15:      base07

! Note: colors beyond 15 might not be loaded (e.g., xterm, urxvt),
! use 'shell' template to set these if necessary
*.color16:      base09
*.color17:      base0F
*.color18:      base01
*.color19:      base02
*.color20:      base04
*.color21:      base06
